{
  "name": "lark-kit",
  "version": "1.0.0",
  "description": "Lightweight TypeScript SDK for Lark (Feishu) API - fetch-only, type-safe, minimal dependencies",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["lark", "feishu", "sdk", "api", "typescript", "fetch", "cloudflare-workers", "edge"],
  "dependencies": {
    "@lark-kit/core": "workspace:*",
    "@lark-kit/shared": "workspace:*",
    "@lark-kit/bitable": "workspace:*",
    "@lark-kit/chat": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
