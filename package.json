{
  "name": "lark-kit",
  "version": "1.0.0",
  "private": true,
  "description": "Lightweight TypeScript SDK for Lark (Feishu) API",
  "license": "MIT",
  "author": "",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "packageManager": "pnpm@8.11.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
